{
    "apiVersion": "1.3",
    "version": "1.0",
    "name": "Rangeplot",
    "id": "rangeplot",
    "icon": "icon.svg",
    
    "properties": [
        {
            "name": "myProperty",
            "type": "string",
            "defaultValue": "myValue"
        },
        {
            "name": "labelVisibility",
            "type": "string",
            "defaultValue": "{\"min\":true,\"max\":true,\"value\":true}"
        }
    ],
    "dataViewDefinition": {
        "tooltip": {
            "enabled": true
        },
        "colorAxis": {
            "mode": "dual",
            "dropTarget": {
                "icon": "Color",
                "description": "Color by {0}"
            }
        },
        "axes": [
            {
                "name": "Range by",
                "mode": "categorical",
                "placement": "top",
                "dropTarget": {
                    "icon": "XAxis3D",
                    "description": "Category by {0}"
                }
            },
            {
                "name": "Min",
                "mode": "continuous",
                "placement": "left",
                "dropTarget": {
                    "icon": "YAxis3D",
                    "description": "Minimum values from {0}"
                }
            },
            {
                "name": "Max",
                "mode": "continuous",
                "placement": "right",
                "dropTarget": {
                    "icon": "YAxis3D",
                    "description": "Maximum values from {0}"
                }
            },
            {
                "name": "Value",
                "mode": "continuous",
                "placement": "bottom",
                "dropTarget": {
                    "icon": "YAxis3D",
                    "description": "Current values from {0}"
                }
            }
        ]
    },
    "files": ["index.html", "main.css", "build/main.js", "src/rangePlot.js", "src/rectangleSelection.js", "src/contextMenu.js"]
}